type byte is 0..255

process _glykoza (&_ADP: byte, &_puryvat: byte, &_glukosa: byte, &_H20: byte, &_NADp: byte, &_Hp: byte, &_NADH: byte, &_ATP: byte) is
states _q

init to _q

from _q
	case ((((_glukosa >= 1) and (_ATP >= 2)) and (_ADP >= 2)) and (_NADp >= 2)) of
	true -> null
	end case;
	_glukosa := (_glukosa - 1);
	_ADP := (_ADP - 2);
	_ATP := (_ATP + 2);
	_NADp := (_NADp - 2);
	_NADH := (_NADH + 2);
	_puryvat := (_puryvat + 2);
	_Hp := (_Hp + 2);
	_H20 := (_H20 + 2);
	to _q


process _puryvat_oxyd (&_puryvat: byte, &_CO2: byte, &_acetyl_co_a: byte, &_NADp: byte, &_NADH: byte) is
states _q

init to _q

from _q
	case ((_puryvat >= 1) and (_NADp >= 1)) of
	true -> null
	end case;
	_puryvat := (_puryvat - 1);
	_NADp := (_NADp - 1);
	_acetyl_co_a := (_acetyl_co_a + 1);
	_NADH := (_NADH + 1);
	_CO2 := (_CO2 + 1);
	to _q


process _krebs_0 (&_CO2: byte, &_acetyl_co_a: byte, &_H20: byte, &_NADp: byte, &_Hp: byte, &_NADH: byte) is
states _oxalacetrat, _citrat, _isocitrat, _oxoglutarat2, _sukcynyl_coa, _sukcynyl, _fumarat, _malat

init to _oxalacetrat

from _oxalacetrat
	case ((_acetyl_co_a >= 1) and (_H20 >= 1)) of
	true -> null
	end case;
	_acetyl_co_a := (_acetyl_co_a - 1);
	_H20 := (_H20 - 1);
	to _citrat

from _citrat
	to _isocitrat

from _isocitrat
	case (_NADp >= 1) of
	true -> null
	end case;
	_NADp := (_NADp - 1);
	_NADH := (_NADH + 1);
	_Hp := (_Hp + 1);
	_CO2 := (_CO2 + 1);
	to _oxoglutarat2

from _oxoglutarat2
	case (_NADp >= 1) of
	true -> null
	end case;
	_NADH := (_NADH + 1);
	_CO2 := (_CO2 + 1);
	to _fumarat

from _fumarat
	case (_H20 >= 1) of
	true -> null
	end case;
	_H20 := (_H20 - 1);
	to _malat

from _malat
	case (_NADp >= 1) of
	true -> null
	end case;
	_NADp := (_NADp - 1);
	_NADH := (_NADH + 1);
	to _oxalacetrat


process _glykolyza [_electrons: out none] (&_O2: byte, &_H20: byte, &_NADp: byte, &_Hp: byte, &_NADH: byte) is
states _q

init to _q

from _q
	case (((_NADH >= 1) and (_O2 >= 1)) and (_Hp >= 2)) of
	true -> null
	end case;
	_electrons;
	_NADH := (_NADH - 1);
	_NADp := (_NADp + 1);
	_O2 := (_O2 - 1);
	_Hp := (_Hp - 2);
	_H20 := (_H20 + 1);
	to _q


process _ETS [_electrons: in none] (&_ADP: byte, &_ATP: byte) is
states _q

init to _q

from _q
	case (_ADP >= 2) of
	true -> null
	end case;
	_electrons;
	_ADP := (_ADP - 2);
	_ATP := (_ATP + 2);
	to _q


process _rest_of_cell (&_energy: byte, &_ADP: byte, &_ATP: byte) is
states _q

init to _q

from _q
	case (_ATP >= 1) of
	true -> null
	end case;
	_ATP := (_ATP - 1);
	_ADP := (_ADP + 1);
	_energy := (_energy + 1);
	to _q


component sys is
var 
	
	_glukosa: byte := 2, 
	
	_ATP: byte := 4, 
	
	_ADP: byte := 4, 
	
	_NADp: byte := 12, 
	
	_NADH: byte := 2, 
	
	_puryvat: byte := 0, 
	
	_acetyl_co_a: byte := 0, 
	
	_O2: byte := 15, 
	
	_CO2: byte := 0, 
	
	_Hp: byte := 10, 
	
	_H20: byte := 10, 
	
	_energy: byte := 0

port 
	_electrons: none

par 
	_glykoza(&_ADP, &_puryvat, &_glukosa, &_H20, &_NADp, &_Hp, &_NADH, &_ATP) 
	 || _puryvat_oxyd(&_puryvat, &_CO2, &_acetyl_co_a, &_NADp, &_NADH) 
	 || _krebs_0(&_CO2, &_acetyl_co_a, &_H20, &_NADp, &_Hp, &_NADH) 
	 || _glykolyza(&_O2, &_H20, &_NADp, &_Hp, &_NADH) 
	 || _ETS(&_ADP, &_ATP) 
	 || _rest_of_cell(&_energy, &_ADP, &_ATP) 
end par


sys
