process _CtoA (&_c_act: byte, &_a: array 18 of byte, &_c: array 18 of byte, &_a_act: byte) is
states _q

init to _q

from _q
	case (_c[(_c_act - 1)] < _a[(_a_act - 1)]) of
	true -> null
	end case;
	_a[_a_act] := _c[(_c_act - 1)];
	_a_act := (_a_act + 1);
