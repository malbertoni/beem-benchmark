type byte is 0..255

process _AtoB (&_b: array 18 of byte, &_a: array 18 of byte, &_b_act: byte, &_a_act: byte) is
states _q

init to _q

from _q
	case (_a[(_a_act - 1)] < _b[(_b_act - 1)]) of
	true -> null
	end case;
	_b[_b_act] := _a[(_a_act - 1)];
	_b_act := (_b_act + 1);
	_a[(_a_act - 1)] := 0;
	_a_act := (_a_act - 1);
	to _q


process _AtoC (&_c_act: byte, &_a: array 18 of byte, &_c: array 18 of byte, &_a_act: byte) is
states _q

init to _q

from _q
	case (_a[(_a_act - 1)] < _c[(_c_act - 1)]) of
	true -> null
	end case;
	_c[_c_act] := _a[(_a_act - 1)];
	_c_act := (_c_act + 1);
	_a[(_a_act - 1)] := 0;
	_a_act := (_a_act - 1);
	to _q


process _BtoA (&_b: array 18 of byte, &_a: array 18 of byte, &_b_act: byte, &_a_act: byte) is
states _q

init to _q

