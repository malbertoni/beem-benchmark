process _Plate_3 [_new_plate: out none, _plt_press_3: out none, _plt_table_3: out none, _plt_belt_3: out none, _plt_deposit_3: out none] (&_done: byte) is
states _new, _wait_belt, _wait_table, _wait_press, _wait_deposit

init to _new

from _new
	_new_plate! 3;
	to _wait_belt

from _wait_belt
	_plt_belt_3;
	to _wait_table

