type byte is 0..255

process _Plate_0 [_new_plate: out none, _plt_press_0: out none, _plt_deposit_0: out none, _plt_table_0: out none, _plt_belt_0: out none] (&_done: byte) is
states _new, _wait_belt, _wait_table, _wait_press, _wait_deposit

init to _new

from _new
	_new_plate! 0;
	to _wait_belt

from _wait_belt
	_plt_belt_0;
	to _wait_table

from _wait_table
	_plt_table_0;
	to _wait_press

from _wait_press
	_plt_press_0;
	to _wait_deposit

from _wait_deposit
	case (_done < 4) of
	true -> null
	end case;
	_plt_deposit_0;
	_done := (_done + 1);
	to _new


process _Plate_1 [_new_plate: out none, _plt_belt_1: out none, _plt_press_1: out none, _plt_deposit_1: out none, _plt_table_1: out none] (&_done: byte) is
