type byte is 0..255

process _P_0 (&_number: array 3 of byte, &_choosing: array 3 of byte) is
states _NCS, _choose, _for_loop, _wait, _CS
var 
	_j: byte, 
	_max: byte 
init to _NCS

from _NCS
	_choosing[0] := 1;
	_j := 0;
	_max := 0;
	to _choose

from _choose
	case ((_j < 3) and (_number[_j] > _max)) of
	true -> null
	end case;
	_max := _number[_j];
	_j := (_j + 1);
	to _choose

from _choose
	case ((_j < 3) and (_number[_j] <= _max)) of
	true -> null
	end case;
	_j := (_j + 1);
	to _choose

from _choose
	case ((_j = 3) and (_max < 6)) of
	true -> null
	end case;
	_number[0] := (_max + 1);
	_j := 0;
	_choosing[0] := 0;
	to _for_loop

from _for_loop
	case ((_j < 3) and (_choosing[_j] = 0)) of
	true -> null
	end case;
	to _wait

from _wait
	case (((_number[_j] = 0) or (_number[_j] < _number[0])) or ((_number[_j] = _number[0]) and (0 <= _j))) of
	true -> null
	end case;
	_j := (_j + 1);
	to _for_loop

from _for_loop
	case (_j = 3) of
	true -> null
	end case;
	to _CS

from _CS
	_number[0] := 0;
	to _NCS


